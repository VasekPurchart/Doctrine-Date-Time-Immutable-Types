{
	"name": "vasek-purchart/doctrine-date-time-immutable-types",
	"description": "Doctrine DateTimeImmutable types",
	"keywords": ["doctrine", "type", "datetime", "immutable", "datetimeimmutable", "dbal"],
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.0",
		"doctrine/dbal": "~2.4"
	},
	"require-dev": {
		"consistence/coding-standard": "1.0",
		"jakub-onderka/php-console-highlighter": "0.3.2",
		"jakub-onderka/php-parallel-lint": "0.9.2",
		"phing/phing": "2.16",
		"phpunit/phpunit": "6.1.3"
	},
	"autoload": {
		"psr-4": { "VasekPurchart\\Doctrine\\Type\\DateTimeImmutable\\": [ "src" ] },
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": { "VasekPurchart\\Doctrine\\Type\\DateTimeImmutable\\": [ "tests" ] },
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
